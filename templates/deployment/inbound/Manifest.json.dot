{
    "spec": "http://eslap.cloud/manifest/deployment/1_0_0",
    "servicename": "eslap://eslap.cloud/services/http/inbound/1_0_0",
    "name": "{{=it.inboundUuid}}",
    "configuration": {
        "resources": {
            "server_cert": null,
            "vhost": "eslap://{{=it.domain}}/resources/vhost/{{=it.inboundUuid}}"
        },
        "parameters": {
            "TLS": false,
            "clientcert": false
        }
    },
    "roles": {
        "sep": {
            "resources": {
                "__instances": 1,
                "__cpu": 1,
                "__memory": 1,
                "__ioperf": 1,
                "__iopsintensive": false,
                "__bandwidth": 1,
                "__resilience": 1
            }
        }
    }
}